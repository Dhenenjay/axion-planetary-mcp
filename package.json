{
    "name": "axion-mcp",
    "version": "1.0.0",
    "description": "Axion MCP Server - AWS-native satellite imagery analysis with SSE transport",
    "private": false,
    "scripts": {
        "setup:check": "node setup-check.js",
        "dev": "tsx watch src/index.ts",
        "dev:aws": "tsx watch src/mcp/server-aws.ts",
        "build": "next build",
        "build:mcp": "tsup src/index.ts --format esm --dts --clean",
        "build:aws": "tsup src/mcp/server-aws.ts --format esm --dts --clean --outDir dist-aws",
        "build:sse": "tsup src/mcp/server-sse.ts --format esm --dts --clean --outDir dist-sse",
        "build:next": "next build",
        "start": "next start -p ${PORT:-3000}",
        "start:mcp": "node dist/index.mjs",
        "start:aws": "tsx src/mcp/server-aws.ts",
        "start:sse": "node dist-sse/server-sse.mjs",
        "dev:sse": "tsx watch src/mcp/server-sse.ts",
        "start:next": "next start",
        "lint": "next lint",
        "test-advanced-earthengine": "node scripts/test-advanced-earthengine.mjs",
        "typecheck": "tsc -p . --noEmit",
        "typecheck:aws": "tsc -p tsconfig.aws.json --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:complete": "node scripts/test-ee-complete.mjs",
        "format": "prettier -w .",
        "test:client": "node scripts/test-ee-tools.mjs",
        "test:health": "curl http://localhost:3000/api/health"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.679.0",
        "@aws-sdk/client-s3": "^3.679.0",
        "@aws-sdk/s3-request-presigner": "^3.679.0",
        "@aws-sdk/util-dynamodb": "^3.948.0",
        "@google-cloud/storage": "^7.17.0",
        "@google/earthengine": "^1.5.14",
        "@modelcontextprotocol/sdk": "^1.11.1",
        "@types/express": "^5.0.6",
        "@types/leaflet": "^1.9.20",
        "@types/uuid": "^10.0.0",
        "@vercel/mcp-adapter": "0.3.1",
        "axios": "^1.11.0",
        "cross-fetch": "^4.1.0",
        "dotenv": "^16.0.0",
        "express": "^5.2.1",
        "geotiff": "^2.1.4-beta.1",
        "google-auth-library": "^10.3.0",
        "googleapis": "^159.0.0",
        "ioredis": "^5.7.0",
        "leaflet": "^1.9.4",
        "ml-cart": "^2.1.1",
        "ml-random-forest": "^2.1.0",
        "nanoid": "^5.1.5",
        "next": "15.2.4",
        "node-fetch": "^3.3.2",
        "pino": "^9.9.4",
        "pino-pretty": "^13.1.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "redis": "^4.7.0",
        "undici": "^7.15.0",
        "uuid": "^13.0.0",
        "vm2": "^3.9.19",
        "xmlhttprequest": "^1.8.0",
        "yaml": "^2.8.1",
        "zod": "^3.24.4"
    },
    "devDependencies": {
        "@types/geojson": "^7946.0.16",
        "@types/node": "^22.18.1",
        "@types/react": "^19",
        "@vitest/coverage-v8": "^3.2.4",
        "eslint": "^8",
        "eslint-config-next": "15.2.4",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "tsup": "^8.5.0",
        "tsx": "^4.20.5",
        "typescript": "^5",
        "vitest": "^3.2.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Dhenenjay/Axion-MCP.git"
    },
    "bugs": {
        "url": "https://github.com/Dhenenjay/Axion-MCP/issues"
    },
    "homepage": "https://github.com/Dhenenjay/Axion-MCP#readme"
}
